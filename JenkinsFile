pipeline {
    agent any
    stages{
        stage('Initaite'){
            steps{
                echo 'Initiation started'
            }
        }
        stage('build'){
            steps{
                echo 'build started'
            }
        }
        stage('test'){
            steps{
                echo 'testing started'
            }
        }
        stage('deploy'){
            steps{
                echo 'deployed the code'
            }
        }
        stage('release'){
            steps{
                echo 'code released to prod'
            }
        }
    }
}
